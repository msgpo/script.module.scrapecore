<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<category label="Dillinger ScrapCore">
		<setting label="Dillinger ScrapCore" type="text" enable="false" />
		<setting label="Provides ScrapCore Services" type="text" enable="false" />
		<setting type="sep"></setting>
		<setting label="Go to http://www.tvaddons.co for support." type="lsep" />
	</category>
	<category label="Premium Services">
		<setting label="Premium Services" type="text" enable="false" />
		<setting label="Priority is applied highest to lowest." type="text" enable="false" />
		<setting label="Premiumize.ME" type="lsep" />
		<setting default="false" id="premiumize_enable" type="bool" label="Enable Premiumize.ME" visible="true" />
		<setting default="100" id="premiumize_priority" type="number" label="Priority" subsetting="true" enable="eq(-1,true)" />
		<setting default="" id="premiumize_username" type="text" label="Customer ID" subsetting="true" enable="eq(-2,true)" />
		<setting default="" id="premiumize_password" type="text" label="Pin" subsetting="true" enable="eq(-3,true)" />
		<setting label="RealDebrid" type="lsep" />
		<setting default="true" id="realdebrid_enable" type="bool" label="Enable RealDebrid" visible="true" />
		<setting default="100" id="realdebrid_priority" type="number" label="Priority" subsetting="true" enable="eq(-1,true)" />
		<setting default="" id="realdebrid_client_id" type="text" visible="false" />
		<setting default="" id="realdebrid_client_secret" type="text" visible="false" />
		<setting default="" id="realdebrid_token" type="text" visible="false" />
		<setting default="" id="realdebrid_refresh_token" type="text" visible="false" />
	</category>
{SCRAPERS_CATEGORY}
</settings>
